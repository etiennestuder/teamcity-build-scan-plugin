<project>
    <modelVersion>4.0.0</modelVersion>

    <groupId>nu.studer.teamcity.buildscan.agent</groupId>
    <artifactId>maven-test-project</artifactId>
    <version>0.0.0</version>

    <!-- Forcing HTTPS is needed for Maven < 3.2.3, where HTTP is the default and Maven Central does not accept HTTP -->
    <repositories>
        <repository>
            <id>central</id>
            <url>https://repo1.maven.org/maven2</url>
            <snapshots>
                <enabled>false</enabled>
            </snapshots>
        </repository>
    </repositories>
    <pluginRepositories>
        <pluginRepository>
            <id>central</id>
            <url>https://repo1.maven.org/maven2</url>
            <snapshots>
                <enabled>false</enabled>
            </snapshots>
        </pluginRepository>
    </pluginRepositories>

</project>
